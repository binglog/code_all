/* pages/aa/aa.wxss */
.container{
    padding: 0rpx 0;
}
#w{
    display: flex;
    flex-direction:column;
    background-color: #f7f7f7;

}
.w{
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    background-color: #f6f6f6;
    justify-content: space-around;
    padding: 10px;
}
.w view{
    /* background-color: red; */
    width: 18%;
    margin-top: 10px;
    text-align: center;
    display: flex;
    flex-direction: column;
}
.w view:nth-child(even){
    /* background-color: pink; */
}
.w image{
    width: 43px;
    height: 43px;
    margin: 0 auto;
}
.w text{
    font-size: 0.6rem;
    margin-top: 5px;
}

/*商品*/
.shangping image{
    width: 100%;
    height: 196px;
    border-radius: 10px 10px 0 0;
}
.shangping view{
    width: 48%;
    display: flex;
    flex-direction: column;
    background-color: #fff;
    margin-bottom: 10px;
    border-radius: 10px;
}
.shangping{
    padding-top: 10px;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
    background-color: #f7f7f7;
}
.shangping .spjs{
    height: 45px;
    text-overflow: clip;
    overflow: hidden;
}
.shangping .jiage{
    font-size: 20px;
    color: red;
}

/*轮播图片*/
.bannerimage image{
    width: 100%;
    height: 100%;
    scale: 0.9;
    border-radius: 10px;
}


/*秒杀*/
.mslan{
    height: 100%;
    width: 90%;
    margin: 0 auto;
    padding: 10px;
    border-radius: 15px;
    overflow: hidden;
    background-color: #fff;
}

.miaosha{
    display: inline-block;
    width: 70px;
    height: 78px;
    flex-direction: column;
    display: inline-flex;
    margin-left: 10px;
}
.miaosha a{
    height: 100%;
}
.miaosha text{
    color: red;
    font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif
}
.miaosha image{
    width: 100%;
    height: 100%;
}
.msbj{
    display: flex;
    justify-content: space-between;
    margin:10px;

    background: url(data:image/png;base64,)no-repeat;
    background-size: contain;
}
.msbj a{
    font-size: 13px;
    color: red;
}
.seckill-more-icon {
    display: inline-block;
    width: 0.55rem;
    height: 0.55rem;
    background: url(//wq.360buyimg.com/wxsq_project/portal/m_jd_index/images/arrow_rt_2ccb8cd5.png) no-repeat;
    background-size: cover;
    right: 0.4rem;
    top: 0.5rem;
}